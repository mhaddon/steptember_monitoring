          "format": "table",
          "rawSql": "SELECT \n  `teams`.`name` as 'team_name', \n  SUM(`subquery`.`distance`) as 'distance', \n  SUM(`subquery`.`steps`) as 'steps', \n  SUM(`subquery`.`calories_burnt`) as 'calories_burnt' \nFROM teams, (\n    SELECT \n      `participants`.`team_id` as 'team_id', \n      `participants`.`name`, \n      MAX(`records`.`distance`) as 'distance', \n      MAX(`records`.`steps`) as 'steps', \n      MAX(`records`.`calories_burnt`) as 'calories_burnt' \n    FROM `records` \n    INNER JOIN `participants` ON `records`.`participant_id`=`participants`.`id` \n    GROUP BY `records`.`participant_id`\n) subquery \nWHERE `teams`.`id` = `subquery`.`team_id` \nGROUP BY `subquery`.`team_id`",
          "refId": "A"
        }
      ],
